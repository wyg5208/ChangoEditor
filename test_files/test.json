      "themes": [
        "dark",
        "light",
        "monokai",
        "solarized"
      ]
    },
    "file_operations": {
      "templates": {
        "python": {
          "extension": ".py",
          "template": "#!/usr/bin/env python3\n\ndef main():\n    pass\n\nif __name__ == '__main__':\n    main()",
          "description": "Python脚本模板"
        },
        "javascript": {
          "extension": ".js",
          "template": "'use strict';\n\nfunction main() {\n    console.log('Hello, World!');\n}\n\nmain();",
          "description": "JavaScript脚本模板"
        },
        "html": {
          "extension": ".html",
          "template": "<!DOCTYPE html>\n<html>\n<head>\n    <title>Document</title>\n</head>\n<body>\n    <h1>Hello, World!</h1>\n</body>\n</html>",
          "description": "HTML文档模板"
        }
      },
      "auto_save": {
        "enabled": false,
        "interval_seconds": 300
      },
      "backup": {
        "enabled": true,
        "max_backups": 5
      }
    },
    "editor": {
      "line_numbers": true,
      "word_wrap": false,
      "auto_indent": true,
      "tab_size": 4,
      "use_spaces": true,
      "show_whitespace": false,
      "font": {
        "family": "Consolas",
        "size": 12,
        "weight": "normal"
      },
      "colors": {
        "background": "#2b2b2b",
        "foreground": "#ffffff",
        "selection": "#0078d4",
        "line_highlight": "#3c3c3c",
        "gutter": "#555555"
      }
    },
    "search_replace": {
      "case_sensitive": false,
      "whole_words": false,
      "regex_enabled": true,
      "highlight_matches": true,
      "max_results": 1000
